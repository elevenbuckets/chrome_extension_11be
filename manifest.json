{
    "name": "Getting Started example",
    "version": "1.0",
    "description": "Build an Extension",
    "permissions": [
        "storage",
        "nativeMessaging"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "11be_logo.png"
    },
    "manifest_version": 2
}